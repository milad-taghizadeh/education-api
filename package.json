{
  "name": "education-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "concurrently \"nest start auth --watch\" \"nest start user --watch\" \"nest start education --watch\" \"nest start enroll --watch\"",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/education-api/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/education-api/test/jest-e2e.json",
    "prisma:generate": "prisma generate --schema ./libs/prisma/prisma/schema.prisma",
    "prisma:push": "prisma db push --schema ./libs/prisma/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema ./libs/prisma/prisma/schema.prisma"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.731.1",
    "@grpc/grpc-js": "^1.12.5",
    "@grpc/proto-loader": "^0.7.13",
    "@nest-lab/throttler-storage-redis": "^1.0.0",
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/microservices": "^10.4.15",
    "@nestjs/passport": "^11.0.4",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/throttler": "^6.3.0",
    "@prisma/client": "^6.2.1",
    "@sentry/node": "^8.50.0",
    "@sentry/profiling-node": "^8.50.0",
    "@sentry/webpack-plugin": "^3.0.0",
    "@types/multer": "^1.4.12",
    "@types/uuid": "^10.0.0",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.5",
    "axios": "^1.7.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "cookieparser": "^0.1.0",
    "dotenv": "^16.4.7",
    "ioredis": "^5.4.2",
    "moment-timezone": "^0.5.46",
    "multer": "^1.4.5-lts.1",
    "opossum": "^8.4.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "soap": "^1.1.7",
    "typeorm": "^0.3.20",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "concurrently": "^9.1.2",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^6.2.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/database(|/.*)$": "<rootDir>/libs/database/src/$1",
      "^@app/prisma(|/.*)$": "<rootDir>/libs/prisma/src/$1",
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1"
    },
    "prisma": {
      "schema": "<rootDir>/libs/prisma/src/prisma/schema.prisma"
    }
  }
}