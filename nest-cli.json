{
  "monorepo": true,
  "root": "apps/education-api",
  "sourceRoot": "apps/education-api/src",
  "compilerOptions": {
    "assets": [
      "**/*.proto"
    ],
    "webpack": true,
    "tsConfigPath": "apps/education-api/tsconfig.app.json"
  },
  "projects": {
    "education-api": {
      "type": "application",
      "root": "apps/education-api",
      "entryFile": "main",
      "sourceRoot": "apps/education-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/education-api/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    },
    "prisma": {
      "type": "library",
      "root": "libs/prisma",
      "entryFile": "index",
      "sourceRoot": "libs/prisma/src",
      "compilerOptions": {
        "tsConfigPath": "libs/prisma/tsconfig.lib.json"
      }
    }
  }
}